            font-size: 14px;
            text-decoration: none;
            margin-top: 20px;
            font-weight: 600;
            letter-spacing: 0.3px;
            transition: all 0.5s ease-out;
        }

            .documentbtn:hover, .documentbtn:active, .documentbtn:focus {
                background-color: #f3c850;
            }


        .digisignLogo {
            width: 220px;
            float: left;
        }

            .digisignLogo img {
                width: 100%;
                float: left;
            }
